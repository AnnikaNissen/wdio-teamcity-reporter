{
  "name": "wdio-teamcity-reporter",
  "version": "1.0.0",
  "description": "WebdriverIO Teamcity reporter",
  "main": "index.js",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "build": "babel index.js --out-file index.js",
    "cleanup": "git reset --hard",
    "prepublish": "in-publish && npm run -s build || in-install",
    "test": "ava --require babel-register test.js",
    "test:n": "ava test.js",
    "test:w": "ava --require babel-register --watch test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sullenor/wdio-teamcity-reporter.git"
  },
  "keywords": [
    "teamcity"
  ],
  "author": "Alexey Litvinov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sullenor/wdio-teamcity-reporter/issues"
  },
  "homepage": "https://github.com/sullenor/wdio-teamcity-reporter#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "in-publish": "^2.0.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "promisify-api": "^1.1.1"
  }
}
